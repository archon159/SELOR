[MESSAGES CONTROL]
disable=invalid-name,redefined-outer-name,too-many-locals,too-many-arguments,too-many-instance-attributes,too-many-statements,too-few-public-methods,duplicate-code,logging-fstring-interpolation

[TYPECHECK]
# List of members which are set dynamically and missed by Pylint inference
# system, and so shouldn't trigger E1101 when accessed.
generated-members=numpy.*, torch.*

[FORMAT]
good-names=i,j,x,x_,y,n,mu,h
